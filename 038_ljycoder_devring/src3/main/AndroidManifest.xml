<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.api.demo">

    <!-- 网络权限DevRing库中已添加，所以这里不必重复添加 -->
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".http.HttpActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".image.ImageActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".db.DBActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".bus.BusActivityA"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".bus.BusActivityB"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".other.OtherActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".util.UtilActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>